RewriteRule ^content/([0-9]+)\-([a-zA-Z0-9-]*) /prestashop/cms.php?id_cms=$1 [QSA,L]
RewriteRule ^content/category/([0-9]+)\-([a-zA-Z0-9-]*) /prestashop/cms.php?id_cms_category=$1 [QSA,L]
RewriteRule ^en/page-not-found$ /prestashop/404.php?isolang=en [QSA,L]
